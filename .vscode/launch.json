    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch image command",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "cwd": "${workspaceFolder}",
            "args": [
                "image", 
                "--path=./sample-accesslog/access.log",
                "--aggregates=/isu/*,/api/condition/*,/?jwt", 
            ]
        },
        {
            "name": "Launch chartjs command",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "cwd": "${workspaceFolder}",
            "args": [
                "chartjs", 
                "--path=./sample-accesslog/access.log",
                "--aggregates=/isu/*,/api/condition/*,/?jwt", 
            ]
        },
    ]
}